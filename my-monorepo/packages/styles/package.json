{
  "name": "@my-monorepo/styles",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "node -e \"const fs=require('node:fs'); fs.mkdirSync('dist',{recursive:true}); fs.writeFileSync('dist/.keep','');\"",
    "typecheck": "node -e \"process.exit(0)\"",
    "lint": "node -e \"process.exit(0)\"",
    "test": "node -e \"process.exit(0)\"",
    "test:cov": "node -e \"process.exit(0)\""
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "**/*.css"
  ],
  "exports": {
    "./global.css": "./global.css",
    "./base/*": "./base/*.css",
    "./layout/*": "./layout/*.css",
    "./themes/*": "./themes/*.css",
    "./tokens/*": "./tokens/*.css"
  }
}
